{
  "name": "zeebe-db-monitor",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "genversion src/version.js && ts-node -r dotenv/config ./src/index.ts",
    "test": "mocha -r ts-node/register ./tests/**/*.test.ts",
    "build": "tsc -p . && genversion build/version.js",
    "build-docker": "npm run build && docker build --tag zeebe-db-monitor . --no-cache",
    "docker-tag": "docker tag zeebe-db-monitor os-docker-registry.epfl.ch/phd-assess-test/zeebe-db-monitor:latest",
    "oc-login": "oc whoami -t | docker login os-docker-registry.epfl.ch -u toto --password-stdin",
    "openshift-push": "docker push os-docker-registry.epfl.ch/phd-assess-test/zeebe-db-monitor:latest",
    "build-and-deploy-on-openshift-test": "npm-run-all oc-login build-docker docker-tag openshift-push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "crypto-js": "^4.1.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "genversion": "^3.1.1",
    "levelup": "^5.1.1",
    "lodash": "^4.17.21",
    "msgpackr": "^1.6.1",
    "prom-client": "^14.0.1",
    "rocksdb": "^5.2.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.0",
    "@types/chai": "^4.3.1",
    "@types/chai-fs": "^2.0.2",
    "@types/debug": "^4.1.7",
    "@types/encoding-down": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/levelup": "^5.1.1",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/rocksdb": "^3.0.1",
    "chai": "^4.3.4",
    "chai-fs": "^2.0.0",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
